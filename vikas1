

float temp;

void setup() 
{
  Serial.begin(9600);
  pinMode(7, OUTPUT);
 
}
void loop() 
{
  temp=analogRead(A2);
  delay(2000);
  temp=temp*0.48828125;
   Serial.print("tempereture=");
   Serial.print(temp); 
   Serial.println(); 
  /*digitalWrite(7, HIGH);
  Serial.print("buzzer ON"); 
  delay(5000);
  Serial.println();             
  digitalWrite(7, LOW); 
  Serial.println("buzzer OFF");           
  delay(5000); */             
     

}
